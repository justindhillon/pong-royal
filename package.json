{
  "name": "pong-royal",
  "version": "1.0.0",
  "private": true,
  "description": "A Multiplayer Pong Battle Royal",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justindhillon/pong-royal.git"
  },
  "scripts": {
    "minify-js": "uglifyjs ./front-end/*.js -o dist/bundle.min.js",
    "minify-css": "cleancss ./front-end/*.css -o dist/styles.min.css",
    "build": "npm run minify-js && npm run minify-css",
    "start": "node index.js"
  },
  "keywords": [
    "javascript",
    "p5js",
    "game",
    "multiplayer"
  ],
  "author": "Justin Dhillon",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/justindhillon/pong-royal/issues"
  },
  "homepage": "https://github.com/justindhillon/pong-royal#readme",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "uglify-js": "^3.17.4"
  }
}
